		map	addnormals(models/weapons/artifact/artifact_local.tga, heightmap(models/weapons/artifact/artifact_petrified_h.tga, 4 ) )
		blend	bumpmap
	}
	specularmap	models/weapons/artifact/artifact_petrified_s.tga
	{
		name	"Stage 4"
		if	parm9
		map	models/weapons/artifact/artifact_helltimefx.tga
		blend	add
		rgb	crazyblink2 [ time * .3 ]
	}
	{
		name	"Stage 5"
		if	parm10
		map	models/weapons/artifact/artifact_berzerkfx.tga
		blend	add
		rgb	crazyblink2 [ time * .3 ]
	}
	{
		name	"Stage 6"
		if	parm11
		map	models/weapons/artifact/artifact_invfx.tga
		blend	add
		rgb	crazyblink2 [ time * .3 ]
	}
}

